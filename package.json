    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/eslint": "^8.2.1",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-reconciler": "^0.26.4",
    "@types/three": "^0.135.0",
    "babel-loader": "^8.2.3",
