{
  "name": "voxeltracer",
  "version": "0.1.0",
  "description": "Blazingly fast voxel ray tracer on web.",
  "repository": "git@github.com:kndlt/voxeltracer.git",
  "author": "kndlt",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
